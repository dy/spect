{
  "name": "spect",
  "version": "23.1.1",
  "description": "Aspect-oriented DOM framework",
  "main": "index.js",
  "module": "index.js",
  "unpkg": "dist/spect.min.js",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./": "./",
    "./dist/": "./dist/"
  },
  "scripts": {
    "test": "node -r ./test/register.cjs --experimental-loader autoesm test/index.js",
    "build": "esbuild index.js --bundle --sourcemap --format=esm --outfile=dist/spect.js",
    "build-min": "esbuild index.js --bundle --minify --sourcemap --format=esm --outfile=dist/spect.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spectjs/spect.git"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "keywords": [
    "dom aspect",
    "aspect",
    "aop",
    "aspect oriented programming",
    "pointcut",
    "join point",
    "advice",
    "observable",
    "useEffect",
    "live collection",
    "HTMLCollection",
    "animationevent",
    "insertionQuery",
    "selector-observer",
    "selector-set",
    "fast-on-load"
  ],
  "author": "dy <df.creative@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spectjs/spect/issues"
  },
  "homepage": "https://github.com/spectjs/spect#readme",
  "devDependencies": {
    "augmentor": "^2.1.5",
    "autoesm": "^1.0.5",
    "htl": "^0.2.0",
    "hyperscript": "^2.0.2",
    "incremental-dom": "^0.7.0",
    "ironjs": "0.0.17-alpha",
    "jsdom": "^16.2.2",
    "observable": "^2.1.4",
    "tst": "^7.0.0",
    "uhtml": "^1.9.1",
    "wait-please": "^3.1.0",
    "xhtm": "^1.5.3"
  },
  "files": [
    "readme.md",
    "src",
    "dist",
    "*.js"
  ],
  "snowpack": {
    "webDependencies": [
      "htm",
      "htl",
      "observable",
      "preact",
      "hyperscript/index.js",
      "lit-html",
      "augmentor",
      "uhtml",
      "incremental-dom"
    ]
  },
  "dependencies": {
    "vref": "^1.1.0"
  }
}
