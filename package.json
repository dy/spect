{
  "name": "spect",
  "version": "23.0.0",
  "description": "Aspect-oriented DOM framework",
  "main": "index.js",
  "module": "index.js",
  "unpkg": "dist/spect.min.js",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./": "./",
    "./dist/": "./dist/"
  },
  "scripts": {
    "start": "es-dev-server --app-index test/index.html --node-resolve --dedupe",
    "test": "node -r ./test/register.cjs --experimental-loader autoesm test/index.js",
    "snowpack": "snowpack",
    "bundle": "rollup -c rollup.config.js",
    "minify": "terser dist/spect.js -c -m --module --source-map -o dist/spect.min.js",
    "build": "npm run bundle && npm run minify",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spectjs/spect.git"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "keywords": [
    "aspect",
    "aop",
    "aspect oriented programming",
    "pointcut",
    "join point",
    "advice",
    "observable",
    "hooks",
    "useEffect",
    "fx",
    "iterator",
    "async iterator",
    "live collection",
    "HTMLCollection",
    "animationevent",
    "insertionQuery",
    "selector-observer",
    "selector-set",
    "fast-on-load",
    "regular-elements",
    "hyperscript",
    "observ",
    "mutant",
    "ironjs",
    "react-hooks",
    "react",
    "preact",
    "rxjs",
    "jquery",
    "jsx",
    "htm"
  ],
  "author": "dy <df.creative@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spectjs/spect/issues"
  },
  "homepage": "https://github.com/spectjs/spect#readme",
  "dependencies": {
    "htm": "^3.0.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "augmentor": "^2.1.5",
    "autoesm": "^1.0.5",
    "es-dev-server": "^1.47.0",
    "esm": "^3.2.25",
    "htl": "^0.2.0",
    "hyperscript": "^2.0.2",
    "incremental-dom": "^0.7.0",
    "ironjs": "0.0.17-alpha",
    "is-module": "^1.0.0",
    "jsdom": "^16.2.2",
    "lit-html": "^1.2.1",
    "nanomorph": "^5.4.0",
    "nodom": "^2.4.0",
    "observable": "^2.1.4",
    "preact": "^10.4.0",
    "rollup": "^2.2.0",
    "snowpack": "^1.6.0",
    "terser": "^4.6.6",
    "tst": "^6.1.1",
    "uhtml": "^1.9.1",
    "unihooks": "^1.4.5",
    "wait-please": "^3.1.0",
    "xhtm": "^1.5.3",
    "zen-observable": "^0.8.15"
  },
  "files": [
    "readme.md",
    "src",
    "dist",
    "*.js"
  ],
  "snowpack": {
    "webDependencies": [
      "htm",
      "htl",
      "observable",
      "preact",
      "hyperscript/index.js",
      "lit-html",
      "augmentor",
      "uhtml",
      "incremental-dom"
    ]
  }
}
